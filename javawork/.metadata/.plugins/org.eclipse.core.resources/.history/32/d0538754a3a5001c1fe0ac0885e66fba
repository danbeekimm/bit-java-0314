package day0317;

public class Ex6ArrayMunje {

	public static void main(String[] args) {
		//
		int []data= {100,-56,67,89,230,-800,32,39,69,11,-9,8,25,33,77};
		int max,min;//최대 최소변수
		max=min=0;
		
		for(int i=0;i<data.length;i++)
			if(data[i] > max)
				max= data[i];
			else min = data[i];
		
		System.out.println("최대값은:"+max);
		System.out.println("최소값은:"+min);
		
	}

}
