package day0315;

public class Ex6Operator {

	public static void main(String[] args) {
		int a=5,b=5,x,y;
		x=a++;//후치증감연산자
		y=++b;//전치증감연산자
		System.out.println(a+","+b);
		System.out.println(x+","+y);
		
		a=b=x=3;
		y=++a+b--*++x;
		System.out.println(y);
		
		a=3;
		System.out.println(a++);//출력후 증가 메모리:4
		System.out.println(++a);//5
		
		
		
		
	}

}
