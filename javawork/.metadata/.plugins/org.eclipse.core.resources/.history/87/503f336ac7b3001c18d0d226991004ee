package day0404;
class Score{
	private int java;
	private int spring;
	//setter method
	
	//getter method
	public void setJava(int java){
		this.java=java;
	}
	
	public void setSpring(int spring){
		this.spring=spring;
	}
	
	public int getJava() {
		return this.java;
	}
	
	public int getSpring() {
		return this.spring;
	}
}
