package day0315;

import java.util.Scanner;

public class Munje {

	public static void main(String[] args) {
		//결과메세지 변수
		//국어점수를 입력후 90이상이면 "Excellent!"라고 msg에 저장
		//90미만이면 "Try!!!"라고 저장
		
		Scanner sc= new Scanner(System.in);
		int kor ;
		String meg;
		
		System.out.println("국어점수 입력하세요");
		kor = sc.nextInt();
		
		String msg;
		if (kor>=90)
			msg = "excellent";
		else 
			msg = "try";
			
			System.out.println("msg");
		
		
		
		
	}

}
