package day0405;

class Color{
	private String colorName;
	
	Color()
	{
		System.out.println("super 디폴트 생성자");
	}
}
class Red extends Color
{
	Red()
	{
		System.out.println("sub 디폴트 생성자");
	}
}
public class Ex4Inheri {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Red r1=new Red();
	}

}
