package day0315;

import java.util.Scanner;

public class Munje {

	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		int a;
		int sum=0;
		int b;
		
		System.out.println("숫자 두개를 입력해주세요");
		a=sc.nextInt();
		b=sc.nextInt();
		
		for (int i=0; i<=b; i++) {
			i= a;
			sum = sum+=a;
			System.out.println("a,b의 합은"+sum);
		}
			
		System.out.println();
		
		}

	}

