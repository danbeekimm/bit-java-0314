package day0404;

class Hello{
	int year,month,day;
	
	Hello(int year,int month,int day){
		this.year=year;
		this.month=month;
		this.day=day;
	}
	
	public void write() {
		System.out.println(year+"년"+month+"월"+day+"일생입니다.");
	}
}

public class Ex11Const {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Hello h=new Hello(2001,4,10);
		h.write();
	}

}
